-- Beep.mif: Memory Initialization File for a simple beep sound
WIDTH=1;           -- data entry 
DEPTH=16;          -- Memory size is 16 words

ADDRESS_RADIX=HEX; -- Address in hexadecimal
DATA_RADIX=BIN;    -- Data in binary

CONTENT BEGIN
  0  :   0; -- Low
  1  :   1; -- High
  2  :   0; 
  3  :   1; 
  4  :   0; 
  5  :   1; 
  6  :   0;
  7  :   1; 
  8  :   0; 
  9  :   1;
  A  :   0; 
  B  :   1; 
  C  :   0; 
  D  :   1; 
  E  :   0; 
  F  :   1; 
END;
